<Window x:Class="_20190604_平方根.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_20190604_平方根"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="540">
    <Grid>
    <StackPanel>
      <Label Content="{Binding ElementName=MySlider, Path=Value}"/>
      <Slider Name="MySlider" Value="3" Minimum="1" Maximum="10" SmallChange="1" TickFrequency="1" IsSnapToTickEnabled="True"/>
      <Button Content="button1" Click="Button_Click"/>
      <Button Content="button2" Click="Button_Click_1"/>
      <Label Name="MyLabelMySqrt" Content="label"/>
      <Label Name="MyLabelSqrt" Content="sqrt"/>
      <Label Name="MyLabelDiff" Content="diff"/>
      <TextBox Name="MyTextBox" Text="65535"/>
      <Button Content="button3" Click="Button_Click_2"/>
      <Button Content="MySqrtTime" Click="Button_Click_3"/>
      <Label Name="MyLabelMySqrtTime" Content="time1"/>
      
      <Button Content="SqrtTime" Click="Button_Click_4"/>
      <Label Name="MyLabelSqrtTime" Content="time2"/>

    </StackPanel>
  </Grid>
</Window>
